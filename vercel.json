{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
 "installCommand": "npm install --legacy-peer-deps --force",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite"
}
